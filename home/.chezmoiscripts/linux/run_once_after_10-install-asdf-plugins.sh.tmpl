#!/bin/bash


export PATH=$HOME/.asdf/bin:$PATH

asdf plugin-add python https://github.com/asdf-community/asdf-python
asdf plugin-add terraform https://github.com/asdf-community/asdf-hashicorp
asdf plugin-add rust https://github.com/code-lever/asdf-rust.git
asdf plugin-add just https://github.com/olofvndrhr/asdf-just
asdf plugin-add yarn https://github.com/twuni/asdf-yarn
asdf plugin-add golang https://github.com/asdf-community/asdf-golang.git
asdf plugin-add neovim https://github.com/richin13/asdf-neovim
asdf plugin-add nodejs https://github.com/asdf-vm/asdf-nodejs.git

asdf install
